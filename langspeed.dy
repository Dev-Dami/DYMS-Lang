import "time" as t
import "fmaths" as math

funct benchmark(label, body) {
    let start = t.millis()
    body()
    let elapsed = t.millis() - start
    println(label + " took " + elapsed + " ms")
}

benchmark("loopTest", funct() {
    for range(i, 2000000) {
        math.sin(i)
    }
})

println("Answer: " + math.sin(30))
